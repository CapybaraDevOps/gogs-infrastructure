variable "region" {
  description = "Region to deploy"
  default     = "europe-west2"
}

variable "zone" {
  description = "Zone to deploy"
  default     = "europe-west2-a"
}

variable "project" {
  description = "Project Id"
  default     = "capybarageocity"
}

variable "storage_class" {
  description = "Storage class name"
  default     = "premium-rwo"
}

